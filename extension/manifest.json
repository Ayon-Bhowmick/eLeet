{
  "manifest_version": 3,
  "name": "eLeet",
  "version": "1.0",
  "description": "Competitive LeetCode",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
